<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/progress_background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="#009d9e9d"
                android:centerColor="#005a5d5a"
                android:centerY="0.75"
                android:endColor="#00747674"
                android:angle="270"
                />
        </shape>
    </item>

    <item android:id="@+id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#12E4FF"
                    android:centerColor="#17E5FF"
                    android:centerY="0.75"
                    android:endColor="#00E2FF"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@+id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#A1231B"
                    android:centerColor="#FF3F31"
                    android:centerY="0.75"
                    android:endColor="#C33025"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>
</layer-list>